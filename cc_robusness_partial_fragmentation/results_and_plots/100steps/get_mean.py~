import sys

inputfile = sys.argv[1]
outputfile = sys.argv[2]

ys = []

with open(inputfile, 'r') as file:
    line = file.readline()
    while line:
        line = file.readline()
        blocks = line.split()
        int_blocks = [float(x) for x in blocks]
        ys.append(int_blocks)
        line = file.readline()

print(len(ys))

mean = list(map(lambda x: float(sum(x))/len(ys), list(zip(*ys))))

with open(outputfile, 'w') as out:
    print(mean, file=out)

